# Raised platform positions and tex coords are transformed by the level they're used in.
usemtl RaisedBottom

# Positions
# Y=0
v 0.0 0.0 0.0
v 1.0 0.0 0.0
v 1.0 0.0 1.0
v 0.0 0.0 1.0

# Normals
# Y=0
vn 0.0 -1.0 0.0
vn 0.0 -1.0 0.0
vn 0.0 -1.0 0.0
vn 0.0 -1.0 0.0

# Tex coords
# Y=0
vt 0.0 0.0
vt 0.0 1.0
vt 1.0 1.0
vt 1.0 0.0

# Indices (1-based)
# Y=0
f 1 2 3
f 3 4 1
